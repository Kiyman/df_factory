{
	"name": "07_D_F_metrics",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "stagingmetricdata",
						"type": "DatasetReference"
					},
					"name": "stagingmetricsream"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "stagingoutput",
						"type": "DatasetReference"
					},
					"name": "stagingoutputstream"
				}
			],
			"transformations": [
				{
					"name": "Selectmetricheaders"
				}
			],
			"scriptLines": [
				"source(output(",
				"          total as integer,",
				"          minimum as integer,",
				"          maximum as integer,",
				"          average as integer,",
				"          resourceId as string,",
				"          time as string,",
				"          metricName as string,",
				"          timeGrain as string,",
				"          count as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     purgeFiles: true,",
				"     documentForm: 'documentPerLine') ~> stagingmetricsream",
				"stagingmetricsream select(mapColumn(",
				"          average,",
				"          time,",
				"          metricName",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Selectmetricheaders",
				"Selectmetricheaders sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> stagingoutputstream"
			]
		}
	}
}